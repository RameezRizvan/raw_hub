  "format_data": "[{\"fieldname\": \"print_heading_template\", \"fieldtype\": \"Custom HTML\", \"options\": \"<style>\\n  body {\\n    font-family: 'Arial', sans-serif;\\n  }\\n\\n  .invoice-container {\\n    text-align: center;\\n    margin-top: 20px;\\n  }\\n  h2 {\\n    margin: 0;\\n    font-size: 24px;\\n    color: #333;\\n  }\\n\\n  .sub-heading {\\n    display: block;\\n    text-align: left;\\n    font-size: 14px;\\n    color: #555;\\n  }\\n\\n  \\n</style>\\n\\n<div class=\\\"invoice-container\\\">\\n  <div class=\\\"print-heading\\\">\\n    <h2>INVOICE</h2>\\n  </div>\\n<h2>\\n    <small class=\\\"sub-heading\\\">Invoice No:     {{ doc.name }}</small><br>\\n    <!--<small class=\\\"sub-heading\\\">Date:       {{ doc.date }}</small>-->\\n  </h2>\\n  <!-- Add the rest of your invoice content here -->\\n</div>\\n\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"date\", \"print_hide\": 0, \"label\": \"Date\"}, {\"fieldname\": \"student\", \"print_hide\": 0, \"label\": \"Student\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldname\": \"_custom_html\", \"print_hide\": 0, \"label\": \"Custom HTML\", \"fieldtype\": \"HTML\", \"options\": \"<style>\\n  .total-box {\\n    border: 1px solid #ddd;\\n    padding: 8px;\\n    width: 292px; /* Adjust the width as needed */\\n    text-align: center;\\n    margin-top: 10px;\\n    height:50px;\\n    margin-left: auto;\\n    display: flex;\\n    justify-content: center;\\n    align-items: center;\\n    \\n  }\\n   table {\\n      width: 100%;\\n      border-collapse: collapse;\\n      margin-top: 20px;\\n    }\\n    \\n\\n    th, td {\\n      border: 1px solid #ddd;\\n      padding: 8px;\\n      text-align: left;\\n    }\\n    \\n    td{\\n        height: 500px;\\n    }\\n\\n    th {\\n      background-color: #f2f2f2;\\n    }\\n\\n    .total {\\n      margin-top: 20px;\\n      float: right;\\n    }\\n</style>\\n<table>\\n      <thead>\\n        <tr>\\n            <th>Sl.No</th>\\n          <th>Title</th>\\n          <th>Amount</th>\\n          <th>Total</th>\\n        </tr>\\n      </thead>\\n      <tbody>\\n        <tr>\\n            <td>1</td>\\n          <td>{{ doc.title}}</td>\\n          <td>{{doc.amount}}</td>\\n          <td>{{doc.amount}}</td>\\n        </tr>\\n      </tbody>\\n</table>\\n\\n        <div class=\\\"total-box\\\">\\n          <h3>Grand Total - \\u20b9 {{ doc.amount }}</h3>\\n        </div>\\n\"}]",
